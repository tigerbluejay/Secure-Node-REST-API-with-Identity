{
  "name": "node-rest-api",
  "version": "1.0.0",
  "description": "NODE REST API project with Express and TypeORM",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "start-server": "node dist/server.js",
    "start-dev-server": "tsc-watch --onSuccess \"node ./dist/src/server.js 9002\"",
    "dev": "npm-run-all clean build start-dev-server",
    "populate-db": "npm-run-all clean build run-populate-db-script",
    "run-populate-db-script": "node dist/src/models/populate-db.js",
    "delete-db": "npm-run-all clean build run-delete-db-script",
    "run-delete-db-script": "node dist/src/models/delete-db.js"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^5.0.5",
    "typeorm": "^0.3.20",
    "winston": "^3.13.0"
  }
}
